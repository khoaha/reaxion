
material elec
{
	technique
	{
		pass
		{
			ambient 0 0 0 1
			lighting off

			texture_unit
			{
				texture textures/elec-blur.png
			}
		}

	}

}



material lightning
{
	technique
	{
		pass
		{
			ambient 0 0 0 1
			scene_blend alpha_blend
			lighting off

			texture_unit
			{
				texture textures/elec.png
			}
		}

	}

}


